ainer,\n  OptionsContainer,\n  OptionLink,\n} from \"./header.styles\";\n// import \"./header.styles.scss\";\n\nimport { signOutStart } from \"../../redux/user/user.actions\";\n\nconst Header = ({ currentUser, hidden, signOutStart }) => (\n  <HeaderContainer>\n    <LogoContainer to=\"/\">\n      <Logo className=\"logo\" />\n    </LogoContainer>\n    <OptionsContainer>\n      <OptionLink to=\"/shop\">SHOP</OptionLink>\n      <OptionLink to=\"/shop\">CONTACT</OptionLink>\n      <OptionLink to=\"/checkout\">CHECKOUT</OptionLink>\n      {currentUser ? (\n        <OptionLink as=\"div\" onClick={signOutStart}>\n          SIGN OUT\n        </OptionLink>\n      ) : (\n        <OptionLink to=\"signin\">SIGN IN</OptionLink>\n      )}\n      <CartIcon />\n    </OptionsContainer>\n    {hidden ? null : <CartDropdown />}\n  </HeaderContainer>\n);\n// BEFORE createStructuredSelector\n// const mapStateToProps = ({ user: { currentUser }, cart: { hidden } }) => ({\n//   currentUser,\n//   hidden,\n// });\nconst mapStateToProps = createStructuredSelector({\n  currentUser: selectCurrentUser,\n  hidden: selectCartHidden,\n});\n\nconst mapDispatchToProps = (dispatch) => ({\n  signOutStart: () => dispatch(signOutStart()),\n});\n\nexport default connect(mapStateToProps, mapDispatchToProps)(Header);\n","/Users/asukanakajima/Desktop/ecomm/crown-shopping/src/pages/sign-in and sign-up/sign-in and sign-up.component.jsx",[],"/Users/asukanakajima/Desktop/ecomm/crown-shopping/src/components/sign-in/sign-in.component.jsx",[],"/Users/asukanakajima/Desktop/ecomm/crown-shopping/src/components/form-input/form-input.component.jsx",[],"/Users/asukanakajima/Desktop/ecomm/crown-shopping/src/components/custom-button/custom-button.component.jsx",[],"/Users/asukanakajima/Desktop/ecomm/crown-shopping/src/firebase/firebase.utils.js",[],"/Users/asukanakajima/Desktop/ecomm/crown-shopping/src/components/sign-up/sign-up.component.jsx",[],"/Users/asukanakajima/Desktop/ecomm/crown-shopping/src/redux/store.js",["226"],"import { createStore, applyMiddleware } from \"redux\";\nimport { persistStore } from \"redux-persist\";\n\n// Middleware - catches action and consolog.log it\nimport logger from \"redux-logger\";\n// [redux thunk -> redux saga] 1)\n// import thunk from \"redux-thunk\";\nimport createSagaMiddleware from \"redux-saga\";\nimport rootSaga from \"./root-saga\";\nimport rootReducer from \"./root-reducer\";\n\n// [redux thunk -> redux saga] 2). createSageMiddleware() can take in an object for other configurations we might need\nconst sagaMiddleware = createSagaMiddleware();\n\nconst middlewares = [sagaMiddleware];\n\nif (process.env.NODE_ENV === \"development\") {\n  middlewares.push(logger);\n}\n\nexport const store = createStore(rootReducer, applyMiddleware(...middlewares));\n\n// [redux thunk -> redux saga] 3) add in the individual saga code (that we write) into the run(ourSagaCodes)\n\nsagaMiddleware.run(rootSaga);\nexport const persistor = persistStore(store);\n\nexport default { store, persistor };\n","/Users/asukanakajima/Desktop/ecomm/crown-shopping/src/redux/root-reducer.js",[],"/Users/asukanakajima/Desktop/ecomm/crown-shopping/src/redux/user/user.reducer.js",["227"],"import UserActionTypes from \"./user.types\";\nimport { auth } from \"../../firebase/firebase.utils\";\n\n// default value\nconst INITIAL_STATE = {\n  currentUser: null,\n  error: null,\n};\n\n// reducer ( currentState, action) => {swtich statement}\n// use default value\nconst userReducer = (state = INITIAL_STATE, action) => {\n  switch (action.type) {\n    case UserActionTypes.SIGN_IN_SUCCESS:\n      return {\n        ...state,\n        currentUser: action.payload,\n        error: null,\n      };\n\n    case UserActionTypes.SIGN_OUT_SUCCESS: {\n      return {\n        ...state,\n        currentUser: null,\n        error: null,\n      };\n    }\n\n    case UserActionTypes.SIGN_IN_FAILURE:\n    case UserActionTypes.SIGN_OUT_FAILURE:\n    case UserActionTypes.SIGN_UP_FAILURE: {\n      return {\n        ...state,\n        error: action.payload,\n      };\n    }\n\n    case UserActionTypes.SIGN_UP_SUCCESS: {\n      return {\n        ...state,\n        currentUser: action.payload,\n        error: null,\n      };\n    }\n    default:\n      return state;\n  }\n};\n\nexport default userReducer;\n","/Users/asukanakajima/Desktop/ecomm/crown-shopping/src/redux/user/user.actions.js",[],"/Users/asukanakajima/Desktop/ecomm/crown-shopping/src/redux/user/user.types.js",[],"/Users/asukanakajima/Desktop/ecomm/crown-shopping/src/components/cart-icon/cart-icon.component.jsx",[],"/Users/asukanakajima/Desktop/ecomm/crown-shopping/src/components/cart-dropdown/cart-dropdown.component.jsx",[],"/Users/asukanakajima/Desktop/ecomm/crown-shopping/src/redux/cart/cart.actions.js",[],"/Users/asukanakajima/Desktop/ecomm/crown-shopping/src/redux/cart/cart.types.js",[],"/Users/asukanakajima/Desktop/ecomm/crown-shopping/src/redux/cart/cart.reducer.js",[],"/Users/asukanakajima/Desktop/ecomm/crown-shopping/src/redux/cart/cart.utils.js",[],"/Users/asukanakajima/Desktop/ecomm/crown-shopping/src/components/cart-item/cart-item.component.jsx",[],"/Users/asukanakajima/Desktop/ecomm/crown-shopping/src/redux/cart/cart.selectors.js",[],"/Users/asukanakajima/Desktop/ecomm/crown-shopping/src/redux/user/user.selector.js",[],"/Users/asukanakajima/Desktop/ecomm/crown-shopping/src/pages/checkout/checkout.component.jsx",[],"/Users/asukanakajima/Desktop/ecomm/crown-shopping/src/components/checkout-item/checkout-item.component.jsx",[],"/Users/asukanakajima/Desktop/ecomm/crown-shopping/src/redux/directory/directory.reducer.js",[],"/Users/asukanakajima/Desktop/ecomm/crown-shopping/src/redux/directory/directory.selectors.js",[],"/Users/asukanakajima/Desktop/ecomm/crown-shopping/src/redux/shop/shop.reducer.js",[],"/Users/asukanakajima/Desktop/ecomm/crown-shopping/src/redux/shop/shop.data.js",[],["228","229"],"/Users/asukanakajima/Desktop/ecomm/crown-shopping/src/redux/shop/shop.selectors.js",[],"/Users/asukanakajim